
config network
        option transmit_interface 'wan'
        option receive_interface 'ifb4wan'
        option transmit_kbits_base '10000'
        option receive_kbits_base '10000'
        option transmit_kbits_min '1500'
        option receive_kbits_min '1500'

config output
        option log_level 'INFO'
        option stats_file '/tmp/sqm-autorate.csv'
        option speed_hist_file '/tmp/sqm-speedhist.csv'

config advanced_settings
        option speed_hist_size '100'
        option reflector_type 'icmp'
        option rtt_delta_bufferbloat '15'
